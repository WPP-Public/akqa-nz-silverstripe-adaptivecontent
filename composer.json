{
  "name": "akqa-nz/silverstripe-adaptivecontent",
  "type": "silverstripe-vendormodule",
  "description": "SilverStripe Adaptive Content",
  "autoload": {
    "psr-4": {
      "Heyday\\AdaptiveContent\\": "src"
    }
  },
  "require": {
    "silverstripe/framework": "^4 | ^5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "3.0.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
